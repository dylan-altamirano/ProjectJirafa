<?xml version="1.0" encoding="windows-1252"?>
<faces-config version="2.0" xmlns="http://java.sun.com/xml/ns/javaee">
  <navigation-rule>
    <from-view-id>/ingreso.jsp</from-view-id>
    <navigation-case>
      <from-outcome>correcto</from-outcome>
      <to-view-id>/principal.jsp</to-view-id>
    </navigation-case>
    <navigation-case>
      <from-outcome>falso</from-outcome>
      <to-view-id>/errorAutenticacion.jsp</to-view-id>
    </navigation-case>
  </navigation-rule>
  <managed-bean>
    <managed-bean-name>beanIngreso</managed-bean-name>
    <managed-bean-class>appProyecto.view.beanIngreso</managed-bean-class>
    <managed-bean-scope>request</managed-bean-scope>
  </managed-bean>
  <managed-bean>
    <managed-bean-name>beanRegistroClientes</managed-bean-name>
    <managed-bean-class>appProyecto.view.beanRegistroClientes</managed-bean-class>
    <managed-bean-scope>request</managed-bean-scope>
  </managed-bean>
  <managed-bean>
    <managed-bean-name>beanRegistroServicios</managed-bean-name>
    <managed-bean-class>appProyecto.view.beanRegistroServicios</managed-bean-class>
    <managed-bean-scope>request</managed-bean-scope>
  </managed-bean>
  <managed-bean>
    <managed-bean-name>beanRegistroTipoServicios</managed-bean-name>
    <managed-bean-class>appProyecto.view.beanRegistroTipoServicios</managed-bean-class>
    <managed-bean-scope>request</managed-bean-scope>
  </managed-bean>
</faces-config>